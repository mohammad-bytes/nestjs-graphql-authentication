{
    "version": 2,
    "env": {
        "PORT": "@PORT",
        "JWT_SECRET": "@JWT_SECRET",
        "DB_URL": "@DB_URL",
        "DB_NAME": "@DB_NAME"
    },
    "builds": [
        {
            "src": "build/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE"
            ]
        }
    ]
}